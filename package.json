{
  "name": "glue-table-cache",
  "version": "0.9.1",
  "description": "Map Glue Table SQL to parquet scan with partition pruned S3 list, along with LRU metadata cache",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "browser": "./dist/browser.js",
        "default": "./dist/index.js"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "yarn build && git add -f dist/"
    }
  },
  "scripts": {
    "release": "standard-version -a",
    "build": "rm -rf dist/ && tsc && node esbuild.config.js",
    "test": "jest",
    "example": "yarn build && NODE_OPTIONS='--loader ts-node/esm' ts-node example.ts",
    "test:unit": "jest test/glue-table-cache.test.ts test/sql-transformer.test.ts",
    "test:integration": "jest test/integration.test.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rimraf dist",
    "all": "yarn format && yarn lint && yarn test"
  },
  "dependencies": {
    "@aws-sdk/client-glue": "^3.496.0",
    "@aws-sdk/client-s3": "^3.496.0",
    "@duckdb/node-api": "^1.1.3-alpha.8",
    "debug": "^4.3.4",
    "jsonpath": "^1.1.1",
    "lru-cache": "^11.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.11",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "aws-sdk-client-mock": "^4.1.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "eslint-plugin-jest": "^28.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "aws",
    "glue",
    "metadata",
    "cache",
    "partition",
    "duckdb"
  ],
  "author": "Dan Forsberg <dforsber@gmail.com>",
  "license": "MIT"
}
